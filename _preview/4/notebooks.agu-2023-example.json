n    ses.SetTimestep(ts)\n    anim.CaptureFrame()\nanim.ShowInteractive()","key":"tofWrhSJmJ"},{"type":"output","id":"htD1kkvqmOJE0NLeq6xXF","data":[],"key":"naGL8f5hIb"}],"key":"RivExboJe0"},{"type":"block","kind":"notebook-content","children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Keyframing","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"O1SXIaHC04"}],"identifier":"keyframing","label":"Keyframing","html_id":"keyframing","implicit":true,"key":"Q3rPcpqTdN"}],"key":"dNmtibV2Zh"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Import necessary modules\n\nfrom vapor import session, camera","key":"ZumQhDEnsb"},{"type":"output","id":"vkGx-j5OlbwgX4mALh82y","data":[],"key":"WtwFHFkzMZ"}],"key":"CE3TC9uBGT"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Acquire the camera settings from an initial session file\n\nses1 = session.Session()\n#ses1.Load(\"/Users/pearse/Examples/eastTroublesome/et_1.vs3\")\nses1.Load(homeDir + \"/et_1.vs3\")\n#ses1.Load(\"~/Examples/eastTroublesome/et_1.vs3\")\n\ncam1 = ses1.GetCamera()\ndir1 = cam1.GetDirection()\npos1 = cam1.GetPosition()\nup1 = cam1.GetUp()","key":"Mn7rzdu6Du"},{"type":"output","id":"lku06GOEg7daWqcAGLxlg","data":[],"key":"uJk1oSIYjF"}],"key":"RMLiHq8y7G"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Acquire the camera settings from a secondary session file that we will transition into\n\nses2 = session.Session()\nses2.Load( homeDir + \"/et_2.vs3\")\ncam2 = ses2.GetCamera()\ndir2 = cam2.GetDirection()\npos2 = cam2.GetPosition()\nup2 = cam2.GetUp()","key":"H9NEe2HrxC"},{"type":"output","id":"GoFcu5PqYWYhhCaJUsm4A","data":[],"key":"LTZX2cJquY"}],"key":"kmhQdKblkq"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Difference between camera positions on each axis\ndPositionX  = (pos2[0] - pos1[0])\ndPositionY  = (pos2[1] - pos1[1])\ndPositionZ  = (pos2[2] - pos1[2])\n\n# Difference between camera direction vectors on each axis\ndDirectionX = (dir2[0] - dir1[0])\ndDirectionY = (dir2[1] - dir1[1])\ndDirectionZ = (dir2[2] - dir1[2])\n\n# Difference between camera up vectors on each axis\ndUpX        = (up2[0] - up1[0])\ndUpY        = (up2[1] - up1[1])\ndUpZ        = (up2[2] - up1[2])","key":"kxd5QmZEpy"},{"type":"output","id":"OaDyVH4s-tp5OgV7fzHFK","data":[],"key":"lIWAzEmpZ1"}],"key":"a53FAGBrbL"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Perform a linear interoplation between the Camera's start position, direction, and up vector\n\nsteps=50\nfor i in range(0,steps):\n    position = [\n        pos1[0]+dPositionX*i/steps,\n        pos1[1]+dPositionY*i/steps,\n        pos1[2]+dPositionZ*i/steps\n    ]\n    cam1.SetPosition( position )\n\n    direction = [\n        dir1[0]+dDirectionX*i/steps,\n        dir1[1]+dDirectionY*i/steps,\n        dir1[2]+dDirectionZ*i/steps\n    ]\n    cam1.SetDirection( direction )\n\n    up = [\n        up1[0]+dUpX*i/steps,\n        up1[1]+dUpY*i/steps,\n        up1[2]+dUpZ*i/steps\n    ]\n    cam1.SetUp( up )\n    ses1.Render( homeDir + \"captures/\" + str(i) + \".png\")","key":"VCjut3Xhtk"},{"type":"output","id":"YKqzqk91DYh5-o0Pnf2Tw","data":[],"key":"baNP1caZ33"}],"key":"dQC4Gl7HP9"},{"type":"block","kind":"notebook-content","children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Camera Orbit","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"V6OE9zQny0"}],"identifier":"camera-orbit","label":"Camera Orbit","html_id":"camera-orbit","implicit":true,"key":"mCqhkKZkkw"}],"key":"RtQpStMaVJ"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Import necessary modules\n\nimport cv2, os\nfrom vapor import session, animation\nfrom numpy import cross, eye, dot, radians, asarray, array\nfrom scipy.linalg import expm, norm\nUseValueFromSessionFile = None","key":"GMyzhVPhoQ"},{"type":"output","id":"K0WC9ow3Tmn4MJklyana0","data":[],"key":"nrY6rMBwuU"}],"key":"uOIxU3XFVP"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Configure various parameters for rotating the camera around the scene\nsession_path = homeDir + \"/et_2.vs3\"\noutput = \"rotation.mp4\"\nvideo_framerate = 30\nvideo_resolution = (640, 480)\ndata_timestep_framerate = 0  # set to zero to disable\nduration = 4  # seconds\nrotate_speed = 45  # deg/s\nrotation_axis = [0,0,1]  # Z (up)\nrotation_center = UseValueFromSessionFile  # Can be replaced with [x,y,z] coordinates here\nsave_individual_frames = False","key":"KjWaIwRCbY"},{"type":"output","id":"NDpi63atogHujvGNcSV7x","data":[],"key":"u4FH2FBE0t"}],"key":"EraQPOWzUD"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Configure our session according to the parameters that were defined above\n\nsession_path, output = [os.path.expanduser(p) for p in (session_path, output)]\nn_frames = video_framerate * duration\n\nses = session.Session()\nses.Load(session_path)\nses.SetResolution(*video_resolution)\ncam = ses.GetCamera()\npos, dir, up, tgt = [asarray(x) for x in [cam.GetPosition(), cam.GetDirection(), cam.GetUp(), cam.GetTarget()]]\nif rotation_center:\n    tgt = asarray(rotation_center)","key":"DH5smFoCwR"},{"type":"output","id":"kjCzUhX7n4YJW31CZPqW3","data":[],"key":"Yga0OpMtgu"}],"key":"JchLxC8x9t"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Define a function that returns a matrix exponential for rotating the camera \n\ndef rotation_matrix(axis, theta):\n    return expm(cross(eye(3), axis / norm(axis) * theta))","key":"cR6vZH4fCx"},{"type":"output","id":"IMsO-2PtXFgUcoK-51yz6","data":[],"key":"XV9gWtKg8X"}],"key":"zbXYlGiMtm"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"anim = animation.Animation(ses)\nfor i in range(0, n_frames):\n    print(f\"Rendering... [{'#'*round(40*i/(n_frames-1))}{' '*round(40*(1-i/(n_frames-1)))}] {100*(i+1)/n_frames:.0f}%\", end=\"\\r\" if i < n_frames-1 else \"\\n\")\n\n    ses.SetTimestep(int(data_timestep_framerate * i / video_framerate))\n\n    M = rotation_matrix(rotation_axis, radians(rotate_speed) * i / video_framerate)\n    cam.SetPosition(dot(M, pos - tgt) + tgt)\n    cam.SetDirection(dot(M, dir))\n    cam.SetUp(dot(M, up))\n    anim.CaptureFrame()\n    if save_individual_frames:\n        ses.Render(f\"{output}_{i:04}.png\")\n\nanim.SaveMP4(output, video_framerate)","key":"G0NXu4kmp1"},{"type":"output","id":"yRBunUSsFskRB4gcnYiAJ","data":[],"key":"Qy5Wqp7n43"}],"key":"Qsa4JHjUDN"}],"key":"AIrEmPQL8F"},"references":{"cite":{"order":[],"data":{}}},"footer":{"navigation":{"prev":{"title":"Custom Map Images with Cartopy","url":"/notebooks/custom-images-example","group":"Foundations of VAPOR Python"},"next":{"title":"Generic Workflow","url":"/notebooks/workflow-example","group":"Example workflows"}}},"domain":"http://localhost:3000"}