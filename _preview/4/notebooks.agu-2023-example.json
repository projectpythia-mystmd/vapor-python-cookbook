n    ses.SetTimestep(ts)\n    anim.CaptureFrame()\nanim.ShowInteractive()","key":"nrio92rPez"},{"type":"output","id":"FwkqI2m5rjFBQu4rikkvn","data":[],"key":"KNkix4InvZ"}],"key":"rBuRSKOkil"},{"type":"block","kind":"notebook-content","children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Keyframing","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"nI7tekdio5"}],"identifier":"keyframing","label":"Keyframing","html_id":"keyframing","implicit":true,"key":"EYqXHxwUUY"}],"key":"JUGI8QBKDj"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Import necessary modules\n\nfrom vapor import session, camera","key":"y6AB5l7Wm9"},{"type":"output","id":"s2gFz2LpYdSTDBb2HJhj1","data":[],"key":"GCIY1Rzw8Z"}],"key":"aUeJLW05VS"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Acquire the camera settings from an initial session file\n\nses1 = session.Session()\n#ses1.Load(\"/Users/pearse/Examples/eastTroublesome/et_1.vs3\")\nses1.Load(homeDir + \"/et_1.vs3\")\n#ses1.Load(\"~/Examples/eastTroublesome/et_1.vs3\")\n\ncam1 = ses1.GetCamera()\ndir1 = cam1.GetDirection()\npos1 = cam1.GetPosition()\nup1 = cam1.GetUp()","key":"XWnYoJUYkR"},{"type":"output","id":"WhkEYVuwcRfCKhaekza_G","data":[],"key":"AVfCRkOOs6"}],"key":"hvNIkiHnl9"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Acquire the camera settings from a secondary session file that we will transition into\n\nses2 = session.Session()\nses2.Load( homeDir + \"/et_2.vs3\")\ncam2 = ses2.GetCamera()\ndir2 = cam2.GetDirection()\npos2 = cam2.GetPosition()\nup2 = cam2.GetUp()","key":"DYeeXkC9wO"},{"type":"output","id":"c9oyp9u39HQMoYpk0yw9o","data":[],"key":"T6ljyNimCw"}],"key":"tkXuDiw4N7"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Difference between camera positions on each axis\ndPositionX  = (pos2[0] - pos1[0])\ndPositionY  = (pos2[1] - pos1[1])\ndPositionZ  = (pos2[2] - pos1[2])\n\n# Difference between camera direction vectors on each axis\ndDirectionX = (dir2[0] - dir1[0])\ndDirectionY = (dir2[1] - dir1[1])\ndDirectionZ = (dir2[2] - dir1[2])\n\n# Difference between camera up vectors on each axis\ndUpX        = (up2[0] - up1[0])\ndUpY        = (up2[1] - up1[1])\ndUpZ        = (up2[2] - up1[2])","key":"dOYiLacnQJ"},{"type":"output","id":"Ds1HukQ6XhcltR5FbYib2","data":[],"key":"tCbPFtMoCr"}],"key":"qZvdtwtWjn"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Perform a linear interoplation between the Camera's start position, direction, and up vector\n\nsteps=50\nfor i in range(0,steps):\n    position = [\n        pos1[0]+dPositionX*i/steps,\n        pos1[1]+dPositionY*i/steps,\n        pos1[2]+dPositionZ*i/steps\n    ]\n    cam1.SetPosition( position )\n\n    direction = [\n        dir1[0]+dDirectionX*i/steps,\n        dir1[1]+dDirectionY*i/steps,\n        dir1[2]+dDirectionZ*i/steps\n    ]\n    cam1.SetDirection( direction )\n\n    up = [\n        up1[0]+dUpX*i/steps,\n        up1[1]+dUpY*i/steps,\n        up1[2]+dUpZ*i/steps\n    ]\n    cam1.SetUp( up )\n    ses1.Render( homeDir + \"captures/\" + str(i) + \".png\")","key":"dbRMYOkTYt"},{"type":"output","id":"8tVwNGbs0NblHQdO2hDBl","data":[],"key":"N9T4tjXqF6"}],"key":"Twb2I4p6qC"},{"type":"block","kind":"notebook-content","children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Camera Orbit","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"uN6Job88ib"}],"identifier":"camera-orbit","label":"Camera Orbit","html_id":"camera-orbit","implicit":true,"key":"NIf4sS2W6P"}],"key":"sBbEiTx6vK"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Import necessary modules\n\nimport cv2, os\nfrom vapor import session, animation\nfrom numpy import cross, eye, dot, radians, asarray, array\nfrom scipy.linalg import expm, norm\nUseValueFromSessionFile = None","key":"MWBORpmRzm"},{"type":"output","id":"PeMkrsiOqWApFgaPdokZ0","data":[],"key":"l13BseL4tR"}],"key":"AamyqvqG6D"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Configure various parameters for rotating the camera around the scene\nsession_path = homeDir + \"/et_2.vs3\"\noutput = \"rotation.mp4\"\nvideo_framerate = 30\nvideo_resolution = (640, 480)\ndata_timestep_framerate = 0  # set to zero to disable\nduration = 4  # seconds\nrotate_speed = 45  # deg/s\nrotation_axis = [0,0,1]  # Z (up)\nrotation_center = UseValueFromSessionFile  # Can be replaced with [x,y,z] coordinates here\nsave_individual_frames = False","key":"sbrPlBIaOO"},{"type":"output","id":"ziPJ1c8-6U5B2PVi4WVAI","data":[],"key":"gyRCXeLcPv"}],"key":"eCJCF7h5iz"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Configure our session according to the parameters that were defined above\n\nsession_path, output = [os.path.expanduser(p) for p in (session_path, output)]\nn_frames = video_framerate * duration\n\nses = session.Session()\nses.Load(session_path)\nses.SetResolution(*video_resolution)\ncam = ses.GetCamera()\npos, dir, up, tgt = [asarray(x) for x in [cam.GetPosition(), cam.GetDirection(), cam.GetUp(), cam.GetTarget()]]\nif rotation_center:\n    tgt = asarray(rotation_center)","key":"aEqHUZmdyp"},{"type":"output","id":"P_1Z2LASeo3PP3JdBAmd-","data":[],"key":"AKFSP9R8jW"}],"key":"SjXIzWkHFf"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# Define a function that returns a matrix exponential for rotating the camera \n\ndef rotation_matrix(axis, theta):\n    return expm(cross(eye(3), axis / norm(axis) * theta))","key":"G7Tpn14rTC"},{"type":"output","id":"iWNEC1sXPzhn6koA3ZEn9","data":[],"key":"ozWoWYpye8"}],"key":"XyR34mi6Ve"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"anim = animation.Animation(ses)\nfor i in range(0, n_frames):\n    print(f\"Rendering... [{'#'*round(40*i/(n_frames-1))}{' '*round(40*(1-i/(n_frames-1)))}] {100*(i+1)/n_frames:.0f}%\", end=\"\\r\" if i < n_frames-1 else \"\\n\")\n\n    ses.SetTimestep(int(data_timestep_framerate * i / video_framerate))\n\n    M = rotation_matrix(rotation_axis, radians(rotate_speed) * i / video_framerate)\n    cam.SetPosition(dot(M, pos - tgt) + tgt)\n    cam.SetDirection(dot(M, dir))\n    cam.SetUp(dot(M, up))\n    anim.CaptureFrame()\n    if save_individual_frames:\n        ses.Render(f\"{output}_{i:04}.png\")\n\nanim.SaveMP4(output, video_framerate)","key":"ys0LHzWwSB"},{"type":"output","id":"PaFhn3jq17Da2YJX-iEO7","data":[],"key":"PcCdhVVyjM"}],"key":"xxPojvrxJt"}],"key":"qB2qzGejs0"},"references":{"cite":{"order":[],"data":{}}},"footer":{"navigation":{"prev":{"title":"Custom Map Images with Cartopy","url":"/notebooks/custom-images-example","group":"Foundations of VAPOR Python"},"next":{"title":"Generic Workflow","url":"/notebooks/workflow-example","group":"Example workflows"}}},"domain":"http://localhost:3000"}